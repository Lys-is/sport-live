<div class="team__wrapper">
    <h1 class="lk_wrapper__title">Матчи</h1>
    <span class="nav_link" data-href="match/get__create">Создать матч</span>
    <table class="team__table">
        <div id="calendarContainer"></div>
    <% if (matches.length > 0) { %>
        <tr>
            <th>Команда 1</th>
            <th>Команда 2</th>
            <th>Время</th>
            <th>Действия</th>
        </tr>
        <% for (let i = 0; i < matches.length; i++) {%>
            <tr class="team__item">
                
                    <td class="team__item__title">
                        <%= matches[i].team_1.name %>
                    </td>
                    <td class="team__item__date">
                        <%= matches[i].team_2.name %>
                    </td>
                    <td class="team__item__creator">
                        <%= matches[i].date %>
                    </td>
                    <td class="team__item__edit">
                        <span class="nav_link" data-href="match/edit/?id=<%= matches[i]._id %>" class="team__item__edit__link">Изменить</span>
                    </td>
            </tr>
        
        <% } %>
    </table>
    <%} else { %>
        <h1 class="lk_wrapper__title">Нет матчей</h1>
    <% } %>
    <script>
        
    </script>
</div>