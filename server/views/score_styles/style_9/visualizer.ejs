      <div class="representative">
        <div style="display: none;" class="away-representative">
          <p class="representative-title">Представители</p>
          <div class="grid"></div>
        </div>
        <div style="display: none;" class="away-couch">
            <p class="representative-title">Тренеры</p>
            <div class="grid"></div>
        </div>    
      </div>
    </div>
  </div>
