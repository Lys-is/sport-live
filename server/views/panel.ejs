    <div class="content-container">
        <div class="column-container">
            <div class="block">
                <div class="h-group center">
                    <p class="first">Матч</p>
                </div>
                <div class="h-group">
                    <select name="match" id="match" class="text-form full-form">
                    <% matches.forEach(match => {
                    %>
                        <option value="<%= match._id %>"><%= match.team_1.name %> - <%= match.team_2.name %></option>
                    <%
                    }); %>
                    </select>
                    <select name="match" id="match" class="text-form full-form">
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                    </select>
                </div>
                <div class="h-group">
                    <input type="button" value="Обновить список" class="full-form" name="match-button-refresh">
                    <input type="button" value="Применить выделенный" class="full-form" name="match-button-accept">
                </div>
            </div>
            

            <div class="block">
                <div class="h-group centered">
                    <p class="first">Вариация табло</p>
                </div>
                <div class="h-group wrap half-form-container">
                    <input type="button" value="Малое" class="selected" name="small-vis">
                    <input type="button" value="Большое" class="" name="big-vis">
                    <input type="button" value="Матч" class="" name="match-vis">
                    <input type="button" value="Сборная 1" class="" name="group-vis">
                    <input type="button" value="Сборная 2" class="" name="mog-vis">
                    <input type="button" value="Пенальти" class="" name="pen-vis">
                    <input type="button" value="Погода" class="" name="weather-vis">
                    <input type="button" value="Выкл" class="" name="off-vis">
                </div>
                <div class="h-group">
                        <label for="panel-style" class="inline-form-label small-text nowrap">Стиль табло</label>
                        <select name="panel-style" id="panel-style" class="text-form full-form">
                            <option value="1">style1</option>
                            <option value="2">style2</option>
                            <option value="3">style3</option>
                            <option value="4">style4</option>
                            <option value="5">style5</option>
                            <option value="6">style6</option>
                            <option value="7">style7</option>
                            <option value="8">style8</option>
                        </select>
                </div>
            </div>
            <div class="block">
                <div class="h-group center">
                    <p class="first">Настройки погоды</p>
                </div>
